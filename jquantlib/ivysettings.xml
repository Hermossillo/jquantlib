<?xml version="1.0"?>
<ivysettings>

	<settings defaultResolver="chain-resolver" />

	<resolvers>
		<url name="jquantlib" m2compatible="true">
			<artifact pattern="${project.deploy.repository.url}/external_free/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			<artifact pattern="${project.deploy.repository.url}/external_non_free/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
		</url>

		<chain name="chain-resolver" dual="false">
			<resolver ref="jquantlib" />
			<ibiblio name="maven2" m2compatible="true" />
		</chain>
	</resolvers>

</ivysettings>

