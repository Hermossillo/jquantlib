  <properties>
    <!-- profile defaults -->
    <profile.manifest.app.realm>default</profile.manifest.app.realm>
    <profile.manifest.app.mode>debug</profile.manifest.app.mode>

    <!-- SCM parameters -->
    <maven.scm.project>jquant</maven.scm.project>
    <maven.scm.branch>trunk</maven.scm.branch>

    <!-- plugin defaults -->
    <maven.buildNumber.doCheck>false</maven.buildNumber.doCheck>
    <maven.compile.jdk.source>1.6</maven.compile.jdk.source>
    <maven.compile.jdk.target>1.6</maven.compile.jdk.target>
    <maven.compile.encoding>UTF8</maven.compile.encoding>
    <maven.compile.meminitial>128m</maven.compile.meminitial>
    <maven.compile.maxmem>512m</maven.compile.maxmem>
    <maven.compile.fork>false</maven.compile.fork> <!-- Be cautious: may fork a different JVM -->
    <maven.jar.index>false</maven.jar.index>
    <maven.jar.manifest.dummy.class>org.jquantlib.examples.Dummy</maven.jar.manifest.dummy.class>
    <maven.jar.manifest.samples.class>org.jquantlib.examples.EquityOptions</maven.jar.manifest.samples.class>

    <!-- application independent properties :: Source Code Management -->
    <maven.scm.root>jquant.svn.sourceforge.net/svnroot</maven.scm.root>
    <maven.scm.connection>scm:svn:http://${maven.scm.root}/${maven.scm.project}/${maven.scm.branch}/${project.artifactId}</maven.scm.connection>
    <maven.scm.developerConnection>scm:svn:https://${maven.scm.root}/${maven.scm.project}/${maven.scm.branch}/${project.artifactId}</maven.scm.developerConnection>
    <maven.scm.url>http://${maven.scm.root}/${maven.scm.project}/${maven.scm.branch}/${project.artifactId}</maven.scm.url>
    
    <maven.deploy.repository.url>http://artifactory.jquantlib.org:8080/artifactory</maven.deploy.repository.url>
    <maven.deploy.site.url>scp://hudson@apache/srv/jquantlib.org/sites</maven.deploy.site.url>
    
  </properties>
 
