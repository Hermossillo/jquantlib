  <distributionManagement>

    <!-- Developer access (privileged) to artifact repository -->
    <repository>
      <id>jquantlib-external_free</id>
      <name>JQuantLib External Free Repository</name>
      <url>${extension.protocol.urlbase}/maven2/repos/external_free</url>
      <layout>default</layout>
      <uniqueVersion>false</uniqueVersion>
    </repository>
    
    <!-- Developer access (privileged) to generated site -->
    <site>
      <id>jquantlib-site</id>
      <name>JQuantLib Generated Site</name>
      <url>${extension.protocol.urlbase}/maven2/sites/${project.artifactId}</url>
    </site>

  </distributionManagement>

  
  <properties>
    <!-- profile defaults -->
    <profile.manifest.app.realm>default</profile.manifest.app.realm>
    <profile.manifest.app.mode>debug</profile.manifest.app.mode>

    <!-- These settings are only needed by Continuum and release managers      -->
    <!-- Defaults used here are intended to avoid problems with maven2.0.9 but -->
    <!-- are not values Continuum and release managers need.                   -->
    <!--  role>default</role>
    <protocol>ssh</protocol -->
    <maven.required.version>2.0.9</maven.required.version>
    <extension.wagon-ssh.version>1.0-beta-1</extension.wagon-ssh.version>
    <extension.wagon-ftp.version>1.0-beta-1</extension.wagon-ftp.version>
    <extension.wagon-file.version>1.0-beta-1</extension.wagon-file.version>
    <extension.wagon-ftp.urlbase>ftp://ftp.jquantlib.org</extension.wagon-ftp.urlbase>
    <extension.wagon-ssh.urlbase>sftp://ftp.jquantlib.org/srv/users/continuum</extension.wagon-ssh.urlbase>
    <extension.wagon-file.urlbase>file://target</extension.wagon-file.urlbase>
    <extension.protocol.urlbase>${extension.wagon-file.urlbase}</extension.protocol.urlbase>


    <!-- SCM parameters -->
    <maven.scm.project>jquant</maven.scm.project>
    <maven.scm.branch>trunk</maven.scm.branch>

    <!-- plugin defaults -->
    <maven.buildNumber.doCheck>false</maven.buildNumber.doCheck>
    <maven.compile.jdk>1.6</maven.compile.jdk>
    <maven.compile.encoding>UTF8</maven.compile.encoding>
    <maven.compile.meminitial>128m</maven.compile.meminitial>
    <maven.compile.maxmem>512m</maven.compile.maxmem>
    <maven.compile.fork>false</maven.compile.fork> <!-- Be cautious: may fork a different JVM -->
    <maven.jar.index>false</maven.jar.index>
    <maven.jar.manifest.mainClass>org.jquantlib.examples.EquityOptions</maven.jar.manifest.mainClass>

    <!-- application independent properties :: Source Code Management -->
    <maven.scm.root>jquant.svn.sourceforge.net/svnroot</maven.scm.root>
    <maven.scm.connection>scm:svn:http://${maven.scm.root}/${maven.scm.project}/${maven.scm.branch}/${project.artifactId}</maven.scm.connection>
    <maven.scm.developerConnection>scm:svn:https://${maven.scm.root}/${maven.scm.project}/${maven.scm.branch}/${project.artifactId}</maven.scm.developerConnection>
    <maven.scm.url>http://${maven.scm.root}/${maven.scm.project}/${maven.scm.branch}/${project.artifactId}</maven.scm.url>
    
  </properties>>
 
