  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav</artifactId>
        <version>1.0-beta-2</version>
      </extension>
    </extensions>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>maven-buildnumber-plugin</artifactId>
        <version>0.9.5</version>
        <executions>
          <execution>
            <phase>validate</phase>
            <goals>
              <goal>create</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <doCheck>${maven.buildNumber.doCheck}</doCheck>
          <doUpdate>${maven.buildNumber.doUpdate}</doUpdate>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-scm-plugin</artifactId>
        <version>1.0-beta-3</version>
        <configuration>
          <goals>install</goals>
          <startDate>YYYY-MM-DD</startDate>
          <endDate>YYYY-MM-DD</endDate>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>${maven.compile.source}</source>
          <target>${maven.compile.target}</target>
          <compilerArguments>
            <bootclasspath>${settings.localRepository}/com/sun/rt/${maven.compile.runtime}/rt-${maven.compile.runtime}.jar</bootclasspath>
          </compilerArguments>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <finalName>${project.artifactId}-${project.version}</finalName>
          <archive>
            <index>${maven.jar.index}</index>
            <manifest>
              <mainClass>${maven.jar.manifest.mainClass}</mainClass>
              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
              <addExtensions>true</addExtensions>
            </manifest>
            <manifestEntries>
              <Build-Jdk>${maven.compile.runtime}</Build-Jdk>
              <Implementation-Build>${buildNumber}</Implementation-Build>
              <App-Realm>${profile.manifest.app.realm}</App-Realm>
              <App-Mode>${profile.manifest.app.mode}</App-Mode>
              <App-URL>${profile.manifest.app.url}</App-URL>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <configuration>
          <descriptorRefs>
            <descriptorRef>jar-with-dependencies</descriptorRef>
          </descriptorRefs>
          <finalName>${project.artifactId}-${project.version}</finalName>
          <appendAssemblyId>false</appendAssemblyId>
          <archive>
            <index>${maven.jar.index}</index>
            <manifest>
              <mainClass>${maven.jar.manifest.mainClass}</mainClass>
            </manifest>
            <manifestEntries-->
              <Build-Jdk>${maven.compile.runtime}</Build-Jdk>
              <Implementation-Build>${buildNumber}</Implementation-Build>
              <App-Realm>${profile.manifest.app.realm}</App-Realm>
              <App-Mode>${profile.manifest.app.mode}</App-Mode>
              <App-URL>${profile.manifest.app.url}</App-URL>
            </manifestEntries-->
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-antrun-plugin</artifactId>
        <configuration>
          <tasks>
            <echo>project.build.directory=${project.build.directory}</echo>
            <echo>project.groupId=${project.groupId}</echo>
            <echo>project.artifactId=${project.artifactId}</echo>
            <echo>project.version=${project.version}</echo>
            <echo>maven.compile.source=${maven.compile.source}</echo>
            <echo>maven.compile.target=${maven.compile.target}</echo>
            <echo>maven.compile.runtime=${maven.compile.runtime}</echo>
            <echo>maven.jar.manifest.mainClass=${maven.jar.manifest.mainClass}</echo>
            <echo>profile.manifest.app.realm=${profile.manifest.app.realm}</echo>
            <echo>profile.manifest.app.mode=${profile.manifest.app.mode}</echo>
            <echo>profile.manifest.app.url=${profile.manifest.app.url}</echo>
          </tasks>
        </configuration>
      </plugin>
    </plugins>
  </build>
  
  <profiles>
    <profile>
      <id>[mode=debug]</id>
      <activation>
        <activeByDefault>true</activeByDefault>
        <property>
          <name>mode</name>
          <value>debug</value>
        </property>
      </activation>
      <properties>
        <profile.manifest.app.mode>debug</profile.manifest.app.mode>
        <maven.buildNumber.doCheck>false</maven.buildNumber.doCheck>
        <maven.buildNumber.doUpdate>false</maven.buildNumber.doUpdate>
      </properties>
    </profile>
    <profile>
      <id>[mode=dev]</id>
      <activation>
        <activeByDefault>true</activeByDefault>
        <property>
          <name>mode</name>
          <value>dev</value>
        </property>
      </activation>
      <properties>
        <profile.manifest.app.mode>dev</profile.manifest.app.mode>
      </properties>
    </profile>
    <profile>
      <id>[mode=test]</id>
      <activation>
        <property>
          <name>mode</name>
          <value>test</value>
        </property>
      </activation>
      <properties>
        <profile.manifest.app.mode>test</profile.manifest.app.mode>
      </properties>
    </profile>
    <profile>
      <id>[mode=prod]</id>
      <activation>
        <property>
          <name>mode</name>
          <value>prod</value>
        </property>
      </activation>
      <properties>
        <profile.manifest.app.mode>prod</profile.manifest.app.mode>
      </properties>
    </profile>
    <profile>
      <id>[realm=default]</id>
      <activation>
        <activeByDefault>true</activeByDefault>
        <property>
          <name>realm</name>
          <value>default</value>
        </property>
      </activation>
      <properties>
        <profile.manifest.app.realm>default</profile.manifest.app.realm>
      </properties>
    </profile>
  </profiles>
  
